lib Slice ;
lib IceUtil ;
lib boost_system ;
lib boost_filesystem ;

lib slicer :
	[ glob *.cpp ]
	:
	<library>Slice
	<library>IceUtil
	<library>boost_system
	<library>boost_filesystem
	<include>..
	<cflags>-fvisibility=hidden
	<variant>release:<cflags>-flto
	: :
	<include>..
	<library>boost_system
	<library>boost_filesystem
	;
